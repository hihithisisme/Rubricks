module rng (
    input level[8],
    output out[3][8]
  ) {

  always {
    out = 3x{{8b0}};
    //out = c{{8b11100000},{8b10000000},{8b11000000}};
    case(level){
    8b1: out = c{{8b11100000},{8b10000000},{8b0}};
    8b10: out = c{{8b10000000},{8b11000000},{8b11000000}};
    8b100: out = c{{8b00000000},{8b11100000},{8b10000000}};
    8b1000: out = c{{8b11000000},{8b00000000},{8b11000000}};
    8b10000: out = c{{8b00000000},{8b00000000},{8b11000000}};
    8b100000: out = c{{8b11000000},{8b10000000},{8b10000000}};
    8b1000000: out = c{{8b11100000},{8b10000000},{8b11000000}};
    8b10000000: out = c{{8b11100000},{8b11100000},{8b11000000}};
    default: out = c{{8b11000000},{8b00000000},{8b11000000}};
    }
  }
}
